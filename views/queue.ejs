        <h2>patients</h2>
      <% patQueue.forEach(patient => {%>
        <li><a href= "admin/<%= patient._id%>" > <%= patient.username %> </a>  <br></li>
    <%  }); %>
    </div>
  </div>

  <div class="column">
    <div class="card">
      <h2>doctor</h2>
      <% doctorQueue.forEach(doctor => {%>
        <li> <a href= "admin/<%= doctor._id%>" > <%= doctor.username %> </a>  <br></li>
    <%  }); %>
    </div>
  </div>
</div>/

    
    

    
    
