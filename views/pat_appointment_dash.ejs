<body style="background-color: rgba(13, 70, 83, 0.78);">
     <a href="/logout">
        <button class="btn btn-danger btn-lg" style="z-index: 1;float:right; margin-top: 0px">Logout</button></a>
   <h2 style="color: white;text-align: center;margin-top: 70px">**doctorlist**</h2>
   <form  class="apt" action="" method="POST" enctype="multipart/form-data">
    <% doctor.forEach(doct=> {%>
        <input type="radio"  name="doctor" value="<%= doct._id %>"><p><%=doct.name%></p>
        <p><%=doct.Specialization%></p>
        <br>

    <%  }); %>
    <br> 
    <p style="color:  : rgba(13, 70, 83, 0.78);">date of appointment</p>
    <input style="color:  : rgba(13, 70, 83, 0.78);" type="date" name="DOA"  required="true"><br>
    <input type="submit" value="request">
</form>


</body>
</html>