<body class="bg">
      <a href="/logout"><button class="btn btn-danger btn-lg" style="z-index: 1;float:right;">Logout</button></a>
      <h1 style="color: white;position: relative; top: 180px;margin-left: 700px;">See Info</h1>
    <div class="row">
  <div class="column">
    <div class="card">
        <h2>patients</h2>
      <% patQueue.forEach(patient => {%>
        <li><a href= "verified/<%= patient._id%>" > <%= patient.username %> </a>  <br></li>
    <%  }); %>
    </div>
  </div>

  <div class="column">
    <div class="card">
      <h2>doctor</h2>
      <% doctorQueue.forEach(doctor => {%>
        <li> <a href= "verified/<%= doctor._id%>" > <%= doctor.username %> </a>  <br></li>
    <%  }); %>
    </div>
  </div>
</div>/

    
    

    
    
